  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 12345 -o",
    "start:local": "ng serve --port 12345 --configuration=local -o",
    "build": "ng build --prod --named-chunks",
    "build:local": "ng build --prod --configuration=local --named-chunks --stats-json",
    "test": "ng test --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "code-scan": "retire --ignorefile .retireignore -p --severity high --nocache -v ",
