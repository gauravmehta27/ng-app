    "bundle-report": "webpack-bundle-analyzer dist/stats-es2015.json --mode static --report ./reports/bundle-analyzer-report.html --no-open",
    "lighthouse:ci": "node_modules/lighthouse/lighthouse-cli/index.js  --output-path=./reports/lighthouse-report.html --quiet  --chrome-flags='--headless'"
  },
